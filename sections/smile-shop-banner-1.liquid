<style>
    .section-container--{{ section.id }}{
      margin-top: {{ section.settings.margin_top | append:'px' }};
      margin-bottom: {{ section.settings.margin_bottom | append:'px' }};
      padding-top: {{ section.settings.padding_top | append:'px' }};
      padding-bottom: {{ section.settings.padding_bottom | append:'px' }};
      overflow: hidden;
      background-image: linear-gradient(to right, #60dbd9 60% , #cfcff5 60%);
    }
    .section-wrapper--{{ section.id }} {
      display: grid;
      grid-template-columns: repeat(2,1fr);
      grid-template-rows: repeat(6,.1fr);
      position: relative;
      margin-block: 50px;
    }

    .section-wrapper--{{ section.id }} p {
        margin-block: 0;
    }
.section-container--{{ section.id }}  .shop-banner-title {
      grid-column: 1/2;
      grid-row: 1/2;
      font-size: 50px;
      line-height: 1.3em;
      font-weight: 600;
      color: #ffff;
  }
.section-container--{{ section.id }}  .shop-banner-title a{
      text-decoration: none;
      color: #ffe180;
      cursor: text;
  }
.section-container--{{ section.id }}  p.shop-banner-subheading_1 {
      grid-column: 1/2;
      grid-row: 2/3;
      color: #5c439a;
      font-size: 17px;
      font-weight: 600;
  }
.section-container--{{ section.id }}  .shop-banner-content {
      grid-column: 1/2;
      grid-row: 3/4;
      color: #000000;
      font-size: 13px;
      font-weight: 400;
      margin-block: 5px;
  }
.section-container--{{ section.id }}  p.shop-banner-subheading_2 {
      grid-column: 1/2;
      grid-row: 4/5;
        color: #5c439a;
      font-size: 13px;
      font-weight: 600;
  }
.section-container--{{ section.id }} .shop-banner-image-wrapper {
      grid-column: 1/2;
      grid-row: 5/6;
  }
.section-container--{{ section.id }} .shop-banner-image-wrapper img {
      max-width: 100%;
  }
.section-container--{{ section.id }} .shop-banner-1-button {
      grid-column: 1/2;
      grid-row: 6/7;
      max-width: fit-content;
  }
.section-container--{{ section.id }}  a.blue-button-1.shop-banner-1-button {
   padding: 7px 50px;
    margin-top: 40px;
    font-size: 16px;
    font-weight: 600;
  }
.section-container--{{ section.id }}  .shop-caption-wrapper {
      display: flex;
      align-items: center;
      z-index: 1;
  }
.section-container--{{ section.id }}  .shop-caption-wrapper  svg{
    max-width: 50px;
    max-height: 50px;
    transform: translateX(-5px);
    z-index: -1;
  }
.section-container--{{ section.id }}  .shop-caption-wrapper p {
      background: #fff;
      border: 1px solid #0000009e;
      border-radius: 20px;
      padding: 10px;
      max-width: 300px;
      color: #000;
      font-size: 12px;
      line-height: 1.2;
  }
.section-container--{{ section.id }}  .shop-caption-1 {
    position: absolute;
    right: 400px;
    top: 20%;
  }
.section-container--{{ section.id }}  .shop-caption-2 {
      position: absolute;
      right: 450px;
      top: 60%;
  }
.section-container--{{ section.id }}  .shop-banner-right-image-wrapper {
      grid-column: 2/-1;
      grid-row: 1/-1;
      text-align: center;
  }
.section-container--{{ section.id }}  .shop-banner-right-image-wrapper img {
      max-width: 100%;
      max-height: 100%;
      transform: scale(1.2) translateY(15px) ;
  }

    @media screen and (max-width: 768px){
      .section-container--{{ section.id }}{
        margin-top: {{ section.settings.margin_top_mobile | append:'px' }};
        margin-bottom: {{ section.settings.margin_bottom_mobile | append:'px'  }};
        padding-top: {{ section.settings.padding_top_mobile | append:'px' }};
        padding-bottom: {{ section.settings.padding_bottom_mobile | append:'px' }};
        background-image: linear-gradient(to bottom, #60dbd9 70% , #cfcff5 60%);
      }
    .section-wrapper--{{ section.id }} {
      display: grid;
      grid-template-columns: repeat(2,1fr);
      /* grid-template-rows: repeat(6,.1fr); */
      position: relative;
      margin-top: 0;
    }
  .section-wrapper--{{ section.id }} p {
      margin-block: 0;
  }
.section-container--{{ section.id }} .shop-banner-title {
      grid-column: 1/-1;
      grid-row: 1/2;
      font-size: 30px;
  }

.section-container--{{ section.id }}  p.shop-banner-subheading_1 {
      grid-column: 1/-1;
      grid-row: 2/3;
  }
.section-container--{{ section.id }} .shop-banner-content {
      grid-column: 1/-1;
      grid-row: 3/4;
  }
.section-container--{{ section.id }} p.shop-banner-subheading_2 {
      grid-column: 1/-1;
      grid-row: 4/5;
  }
.section-container--{{ section.id }} .shop-banner-image-wrapper {
      grid-column: 1/-1;
      grid-row: 5/6;
  }
.section-container--{{ section.id }} .shop-banner-image-wrapper img {
      width: 100%;
      margin-block: 5px;
  }
.section-container--{{ section.id }} .shop-banner-right-image-wrapper {
      grid-column: 2/-1;
      grid-row: 6/7;
      text-align: center;
      min-height: 300px;
      align-self: end;
  }
.section-container--{{ section.id }} .shop-banner-right-image-wrapper img {
      transform: scale(2) translateY(60px) translateX(-10px);
  }
.section-container--{{ section.id }} .shop-banner-1-button {
      grid-column: 1/2;
      grid-row: 6/7;
      max-width: fit-content;
  }
.section-container--{{ section.id }}  a.blue-button-1.shop-banner-1-button {
      height: fit-content;
      align-self: end;
      padding: 7px 30px;
      margin-top: 40px;
      font-size: 14px;
      font-weight: 600;
  }
     
.section-container--{{ section.id }}  .shop-caption-wrapper {
      max-width: 250px;
  }
.section-container--{{ section.id }}  .shop-caption-1 {
    position: absolute;
    right:100px;
    top:350px;
  }
.section-container--{{ section.id }}  .shop-caption-2 {
    position: absolute;
    right:70px;
    top:450px;
  }

    }

@media screen and (max-width: 768px){
   .section-wrapper--{{ section.id }}.page-width {
      padding-inline: 1.5rem;
    }
  .section-container--{{ section.id }}  .shop-caption-1 {
    right: 100px;
}
.section-container--{{ section.id }}  .shop-caption-2 {
    right: 70px;
}
 }  
 @media screen and (min-width: 768px) and (max-width: 1200px){ 
.section-wrapper--{{ section.id }}.page-width {
padding-inline: 10rem;
}
.section-container--{{ section.id }}  .shop-caption-1 {
right: 300px;
}
.section-container--{{ section.id }}  .shop-caption-2 {
right: 350px;
}
}  
 @media screen and (min-width: 1200px) and (max-width: 1400px){ 
.section-wrapper--{{ section.id }}.page-width {
padding-inline: 10rem;
}
.section-container--{{ section.id }}  .shop-caption-1 {
right: 350px;
}
.section-container--{{ section.id }}  .shop-caption-2 {
right: 400px;
}
}  
 @media screen and (min-width: 1400px) and (max-width: 1600px){ 
.section-wrapper--{{ section.id }}.page-width {
padding-inline: 10rem;
}
 .section-container--{{ section.id }}  .shop-caption-1 {
right: 400px;
}
.section-container--{{ section.id }}  .shop-caption-2 {
right: 450px;
}
}  
 @media screen and (min-width: 1601px) { 
.section-wrapper--{{ section.id }}.page-width {
padding-inline: 20rem;
}
   .section-container--{{ section.id }}  .shop-caption-1 {
right: 30%;
}
.section-container--{{ section.id }}  .shop-caption-2 {
right: 35%;
}
}    
</style>
<section class="section-container--{{ section.id }}">
  <div class="section-wrapper--{{ section.id }} page-width">
    <div class="shop-banner-title">{{ section.settings.title }}</div>
    <p class="shop-banner-subheading_1">{{ section.settings.subheading_1 }}</p>
    <div class="shop-banner-content">{{ section.settings.content }}</div>
    <p class="shop-banner-subheading_2">{{ section.settings.subheading_2 }}</p>
    {% unless section.settings.image == blank %}
      <div class="shop-banner-image-wrapper">
        {{ section.settings.image | img_url: 'master' | img_tag }}
      </div>
    {% endunless %}
    <a class="blue-button-1 shop-banner-1-button" href="{{ section.settings.button_url }}">
      {{ section.settings.button_text }}
    </a>
    {% for block in section.blocks %}
      <div data-aos="fade-right" class=" shop-caption-1 shop-caption-wrapper">
        {{ block.settings.caption_1 }}
        {% render 'smile-all-icons', name: 'caption_new' %}
      </div>
      <div data-aos="fade-right" class="shop-caption-2 shop-caption-wrapper">
        {{ block.settings.caption_2 }}
        {% render 'smile-all-icons', name: 'caption_new' %}
      </div>
      <div class="shop-banner-right-image-wrapper">
        {{ block.settings.image | img_url: 'master' | img_tag }}
      </div>
    {% endfor %}
  </div>
</section>

{% schema %}
{
  "name": "Smile Shop Banner-1",
  "settings": [
    {
      "type": "richtext",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "text",
      "id": "subheading_1",
      "label": "Sub Heading 1"
    },
    {
      "type": "richtext",
      "id": "content",
      "label": "Content"
    },
    {
      "type": "text",
      "id": "subheading_2",
      "label": "Sub Heading 2"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Button Text"
    },
    {
      "type": "url",
      "id": "button_url",
      "label": "Button Url"
    },
    {
      "type": "header",
      "content": "Desktop Spacing"
    },
    {
      "type": "range",
      "id": "margin_top",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Margin Top",
      "default": 10
    },
    {
      "type": "range",
      "id": "margin_bottom",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Margin Bottom",
      "default": 10
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Padding Top",
      "default": 50
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Padding Bottom",
      "default": 50
    },
    {
      "type": "header",
      "content": "Mobile Spacing"
    },
    {
      "type": "range",
      "id": "margin_top_mobile",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Margin Top (Mobile)",
      "default": 10
    },
    {
      "type": "range",
      "id": "margin_bottom_mobile",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Margin Bottom (Mobile)",
      "default": 10
    },
    {
      "type": "range",
      "id": "padding_top_mobile",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Padding Top  (Mobile)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_bottom_mobile",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "Padding Bottom  (Mobile)",
      "default": 20
    }
  ],
  "blocks": [
    {
      "name": "Image Block",
      "type": "image_block",
      "limit": 1,
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image"
        },
        {
          "type": "richtext",
          "id": "caption_1",
          "label": "Caption 1"
        },
        {
          "type": "richtext",
          "id": "caption_2",
          "label": "Caption 2"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Smile Shop Banner-1"
    }
  ]
}
{% endschema %}
